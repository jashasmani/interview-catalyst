{
   "version": 2,
   "builds": [
       {
           "src": "./index.js",
           "use": "@vercel/node"
       }
   ],
   "routes": [  
       {
           "src": "/",
           "methods":["POST","GET"],
           "dest": "/index.js"
       },
       {
           "src": "/users",
           "methods":["POST","GET"],
           "dest": "/index.js"
       }
   ]
}